@model IplusOAEntity.VolunteerEntity


<script type="text/javascript">
    function check_incharge_form() {

        return true;
    }
</script>
<div class="main">
    <div class="main_title">
        用户信息
    </div>
    <div class="blank5"></div>
    <form name="edit" action="@Url.Action("PostEdit")" method="post" enctype="multipart/form-data" onsubmit="return check_incharge_form();">
        <table class="form" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <td colspan="2" class="topTd"></td>
                </tr>

                <tr>
                    <td class="item_title">用户编号:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="Number" value="@Model.Number" /></td>
                </tr>
                <tr>
                    <td class="item_title">类型:</td>
                    <td class="item_input"> <input disabled="disabled" type="text" class="textbox require" name="Number" value="@Model.Type" /> </td>
                </tr>
                <tr>
                    <td class="item_title">社区编号:</td>
                    <td class="item_input"><input disabled="disabled"  type="text" class="textbox require" name="DepId" value="@Model.DepId" /></td>
                </tr>
                <tr>
                    <td class="item_title">居住地址:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="Address" value="@Model.Address" /></td>
                </tr>
                <tr>
                    <td class="item_title">实名制状态:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="RealNameState" value="@Model.RealNameState" /></td>
                </tr>
                <tr>
                    <td class="item_title">真实姓名:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="RealName" value="@Model.RealName" /></td>
                </tr>
                <tr>
                    <td class="item_title">证件类型:</td>
                    <td class="item_input"> <input disabled="disabled" value="@Model.CardType"></td>
                </tr>
                <tr>
                    <td class="item_title">证件编号:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="CardNum" value="@Model.CardNum" /></td>
                </tr>
                <tr>
                    <td class="item_title">自定义名称:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="UerName" value="@Model.UerName" /></td>
                </tr>
                <tr>
                    <td class="item_title">信誉指数:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="Honor" value="@Model.Honor" /></td>
                </tr>
                <tr>
                    <td class="item_title">联系电话:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="Phone" value="@Model.Phone" /></td>
                </tr>
                <tr>
                    <td class="item_title">邮件:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="EMail" value="@Model.EMail" /></td>
                </tr>
                <tr>
                    <td class="item_title">QQ号码:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="QQ" value="@Model.QQ" /></td>
                </tr>
                <tr>
                    <td class="item_title">微信号:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="WeiXin" value="@Model.WeiXin" /></td>
                </tr>
                <tr>
                    <td class="item_title">群体ID:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="GroupID" value="@Model.GroupID" /></td>
                </tr>
                <tr>
                    <td class="item_title">感恩卡:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="ThsScore" value="@Model.ThsScore" /></td>
                </tr>
                <tr>
                    <td class="item_title">积分:</td>
                    <td class="item_input"><input disabled="disabled"  type="text" class="textbox require" name="Score" value="@Model.Score" /></td>
                </tr>
                <tr>
                    <td class="item_title">志愿者编号:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox require" name="VID" value="@Model.VID" /></td>
                </tr>
                <tr>
                    <td class="item_title">社会组织编号:</td>
                    <td class="item_input"><input disabled="disabled" type="text" class="textbox" name="SocialNO" value="@Model.SocialNO" /></td>
                </tr>
                <tr>
                    <td class="item_title">审核:</td>
                    <td class="item_input">
                        <input   type="radio"  class="textbox require" name="State"  value="@Model.State" />通过
                        <input    type="radio"  class="textbox require" name="State" value="-1" />不通过
                    </td>
                </tr>
                <tr>
                    <td class="item_title">原因:</td>
                    <td class="item_input">
                        <textarea   class="textbox require" name="Msg" rows="4" cols="40"  >@Model.Msg </textarea>
                      
                    </td>
                </tr>
                <tr>
                    <td class="item_title"></td>
                    <td class="item_input">
                        <input  type="hidden" value="@Model.Id" name="Id"/>
                        <input  type="submit" class="button" value="提交审核">
                       
                    </td>
                </tr>
                <tr>
                    <td colspan="2" class="bottomTd"></td>
                </tr>
            </tbody>
        </table>
    </form>
</div>