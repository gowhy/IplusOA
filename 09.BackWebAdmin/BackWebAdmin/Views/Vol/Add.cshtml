

<script type="text/javascript">
    function check_incharge_form() {

        return true;
    }
</script>
<div class="main">
    <div class="main_title">
        自愿者注册
    </div>
    <div class="blank5"></div>
    <form name="edit" action="@Url.Action("PostAdd")" method="post" enctype="multipart/form-data" onsubmit="return check_incharge_form();">
        <table class="form" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <td colspan="2" class="topTd"></td>
                </tr>
                <tr>
                    <td class="item_title">用户编号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="Number" /></td>
                </tr>
                <tr>
                    <td class="item_title">类型:</td>
                    <td class="item_input">
                        <select name="Type">
                            <option value="普通用户">普通用户</option>
                            <option value="志愿者">志愿者</option>                    
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="item_title">社区编号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="DepId" /></td>
                </tr> 
                <tr>
                    <td class="item_title">居住地址:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="Address" /></td>
                </tr>
                <tr>
                    <td class="item_title">实名制状态:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="RealNameState" /></td>
                </tr>
                <tr>
                    <td class="item_title">真实姓名:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="RealName" /></td>
                </tr>
                <tr>
                    <td class="item_title">证件类型:</td>
                    <td class="item_input">
                        <select name="CardType">
                            <option value="身份证">身份证</option>
                            <option value="学生证">学生证</option>
                            <option value="士兵证">士兵证</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="item_title">证件编号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="CardNum" /></td>
                </tr>
                <tr>
                    <td class="item_title">自定义名称:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="UerName" /></td>
                </tr>
                <tr>
                    <td class="item_title">信誉指数:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="Honor" /></td>
                </tr>
                <tr>
                    <td class="item_title">联系电话:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="Phone" /></td>
                </tr>
                <tr>
                    <td class="item_title">邮件:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="EMail" /></td>
                </tr>
                <tr>
                    <td class="item_title">QQ号码:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="QQ" /></td>
                </tr>
                <tr>
                    <td class="item_title">微信号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="WeiXin" /></td>
                </tr>
                <tr>
                    <td class="item_title">群体ID:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="GroupID" /></td>
                </tr>
                <tr>
                    <td class="item_title">感恩卡:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="ThsScore" /></td>
                </tr>
                <tr>
                    <td class="item_title">积分:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="Score" /></td>
                </tr>
                <tr>
                    <td class="item_title">志愿者编号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="VID" /></td>
                </tr>
                <tr>
                    <td class="item_title">社会组织编号:</td>
                    <td class="item_input"><input type="text" class="textbox require" name="SocID" /></td>
                </tr>
                <tr>
                    <td class="item_title"></td>
                    <td class="item_input">
                        <input type="submit" class="button" value="新增">
                        <input type="reset" class="button" value="重置">
                    </td>
                </tr>
                <tr>
                    <td colspan="2" class="bottomTd"></td>
                </tr>
            </tbody>
        </table>
    </form>
</div>